{
    "name": "",
    "license": "MIT",
    "dota_developer": {
        "main_lang": "SChinese",
        "publish_options": {
            "excludeFiles": [
                "**/*.ts",
                "**/*.bin",
                "**/*.py",
                "**/*.json",
                "**/*.cfg"
            ],
            "encryptFiles": [
                "**/*.lua",
                "!game/scripts/vscripts/addon_init.lua"
            ],
            "encryptDedicatedServerKey": "Invalid_NotDedicatedServer"
        }
    },
    "scripts": {
        "launch": "node scripts/launch.js",
        "preprod": "node scripts/prepublish.js",
        "prod": "run-s prod:*",
        "prod:panorama": "node --preserve-symlinks node_modules/webpack/bin/webpack.js --config content/panorama/webpack.config.js",
        "prod:vscripts": "tstl --project game/scripts/tsconfig.json",
        "dev": "run-p dev:*",
        "dev:panorama": "node --preserve-symlinks node_modules/webpack/bin/webpack.js --config webpack.config.js --watch",
        "dev:js": "tsc -p tsconfig.json --watch "
    },
    "devDependencies": {
        "@moddota/dota-lua-types": "^4.7.1",
        "@moddota/panorama-types": "^1.5.0",
        "@types/react": "^16.14.5",
        "aes-js": "^3.1.2",
        "body-parser": "^1.19.0",
        "chokidar": "^3.5.1",
        "commander": "^6.2.1",
        "crypto-js": "^4.0.0",
        "express": "^4.17.1",
        "file-loader": "^6.2.0",
        "file-match": "^1.0.2",
        "find-steam-app": "^1.0.2",
        "fork-ts-checker-webpack-plugin": "^5.2.1",
        "fs-extra": "^9.1.0",
        "keyvalues-node": "0.0.2",
        "node-sass": "^4.14.1",
        "node-xlsx": "^0.15.0",
        "npm-run-all": "^4.1.5",
        "react": "^16.14.0",
        "react-panorama": "^0.1.1",
        "rimraf": "^3.0.2",
        "sass": "^1.32.8",
        "sass-loader": "^10.1.1",
        "ts-loader": "^8.0.18",
        "typescript": "^3.9.9",
        "typescript-to-lua": "^0.39.0",
        "walk": "^2.3.14",
        "webpack": "^5.0.0-beta.30",
        "webpack-cli": "^3.3.12",
        "webpack-panorama": "^0.2.0"
        "rd": "^2.0.1"
    }
}
